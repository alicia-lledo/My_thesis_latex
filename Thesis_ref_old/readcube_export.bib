  author={Reich, Kristian and Papp, Kim A and Matheson, Robert T and Tu, John H and Bissonnette, Robert and Bourcier, Marc and Gratton, David and Kunynetz, Rodion A and Poulin, Yves and Rosoph, Les A and Stingl, Georg and Bauer, Wolfgang M and Salter, Janeen M and Falk, Thomas M and {Bl{\"o}dorn‐Schlicht,} Norbert A and Hueber, Wolfgang and Sommer, Ulrike and Schumacher, Martin M and Peters, Thomas and Kriehuber, Ernst and Lee, David M and Wieczorek, Grazyna A and Kolbinger, Frank and Bleul, Conrad C},
