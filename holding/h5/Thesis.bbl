  \verb 10.1038/ni.1698
  \endverb
  \field{issn}{1529-2908}
  \field{number}{3}
  \field{pages}{314\bibrangedash 24}
  \field{title}{The interleukin 23 receptor is essential for the terminal
  differentiation of interleukin 17-producing effector T helper cells in vivo.}
  \field{volume}{10}
  \field{journaltitle}{Nat. Immunol.}
  \field{year}{2009}
\endentry

